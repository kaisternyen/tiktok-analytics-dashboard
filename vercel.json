{
    "build": {
        "env": {
            "NEXT_TELEMETRY_DISABLED": "1"
        }
    },
    "functions": {
        "src/app/api/*/route.ts": {
            "maxDuration": 60
        },
        "src/app/api/scrape/route.ts": {
            "maxDuration": 30
        },
        "src/app/api/scrape-all/route.ts": {
            "maxDuration": 60
        }
    },
    "regions": [
        "iad1"
    ],
    "crons": [
        {
            "path": "/api/scrape-all",
            "schedule": "*/5 * * * *"
        }
    ]
}